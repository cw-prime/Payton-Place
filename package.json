    "frontend": "npm run dev --workspace=frontend"
  },
  "keywords": [
    "real-estate",
    "consulting",
    "renovation",
    "development"
  ],
  "author": "Payton Place Development",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
